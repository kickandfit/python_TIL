    "import pandas as pd\n",
    "import csv\n",
    "\n",
    "f=open('./Data/seoul.csv','r', encoding='cp949') \n",
    "# 엑셀을 열었는데 한글이 보이면 cp949\n",
    "data = csv.reader(f, delimiter=\",\") # 안에 있는 데이터를 , 단위로 구분해서 가져와라\n",
    "\n",
    "print(data)\n",
    "f.close()"
